{
  "manifest_version": 2,
  "version": "0.1.5",
  "name": "ZIP Manager",
  "description": "Open local and remote ZIP archives to extract all files or a single one",
  "permissions": [
    "storage",
    "downloads",
    "contextMenus",
    "*://*/*"
  ],
  "offline_enabled": true,
  "icons": {
    "16": "data/icons/16.png",
    "19": "data/icons/19.png",
    "32": "data/icons/32.png",
    "38": "data/icons/38.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png"
  },
  "browser_action": {},
  "homepage_url": "https://add0n.com/zip-manager.html",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
